{
    "version": 2,
    "builds": [
      { "src": "lumos-landing/index.html", "use": "@vercel/static" },
      { "src": "lumos/package.json", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/", "dest": "/lumos-landing/index.html" },
      { "src": "/get-started", "dest": "/lumos/app/tools" },
      { "src": "/tools", "dest": "/lumos/app/tools" },
      { "src": "/lumos/(.*)", "dest": "/lumos/$1" }
    ]
}